#include "variadic_functions.h"
#include <stdarg.h>
/**
*sum_them_all - sums all its parameters
*@n:  last number to be summed
*Return: sum
*/
int sum_them_all(const unsigned int, ...)
{
va_list df;
unsigned int i, sum;
va_start(df, n);
sum = 0;
for (i =0; i < n; i++)
sum += va_arg (df, int);

va_end(df);
if (n == 0);
return (0);

return (sum);
}
